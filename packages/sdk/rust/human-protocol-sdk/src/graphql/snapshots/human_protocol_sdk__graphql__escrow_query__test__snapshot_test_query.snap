---
source: src/graphql/escrow_query.rs
expression: query.query
---
query GetEscrowByAddressQuery($id: String!) {
  escrow(id: $id) {
    id
    address
    amountPaid
    balance
    count
    factoryAddress
    launcher
    status
    token
    totalFundedAmount
    createdAt
    finalResultsUrl
    intermediateResultsUrl
    manifestHash
    manifestUrl
    recordingOracle
    recordingOracleFee
    reputationOracle
    reputationOracleFee
    exchangeOracle
    exchangeOracleFee
  }
}


